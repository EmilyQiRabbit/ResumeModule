{
  "name": "resume_template",
  "version": "0.1.0",
  "private": true,
  "author": "Yuqi <LiuYQEmily@163.com>",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "build": "tsc"
  },
  "dependencies": {
    "ejs": "^2.7.1",
    "koa": "2.7.0",
    "koa-router": "7.4.0",
    "koa-views": "^6.2.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@types/node": "10.12.18",
    "@types/koa": "2.0.48",
    "@types/koa-router": "7.0.35",
    "husky": "^1.1.4",
    "prettier": "^1.18.2",
    "lint-staged": "^8.0.5",
    "typescript": "3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,ts,tsx,json,css,less,scss,md}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/assets/**/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
